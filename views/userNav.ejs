@media (max-width:1100px) {
    .menu {
        position: absolute;
        justify-content: flex-start;
        align-items: center;
        top: 0;
        right: 0;
        padding: 0;
        padding-top: 4rem;
        background-image: radial-gradient(circle, #dcdcdc, #e5e5e5, #ededed, #f6f6f6, #ffffff);
        background-color: rgb(212, 212, 212);
        width: 40vw;
        height: 35rem;
        margin: 0;
        border-bottom-left-radius: 800px;
        flex-direction: column;
        opacity: 0;
        pointer-events: none;
        visibility: hidden;
        transition: all 0.5s ease-in-out;
    }
    .menu>li>a {
        font-weight: 600;
        color: #000;
        top: 0;
    }

    .btn1 {
        display: block;
        
    }

    .close-icon {
        display: none;

    }

    .nav-open .menu {
        opacity: 1;
        pointer-events: auto;
        visibility: visible;
        
    }

    .nav-open .close-icon {
        display: block;
        position: absolute;
        top: 0;
        filter: invert(0%) sepia(0%) saturate(0%) hue-rotate(320deg) brightness(0%) contrast(104%);
        right: 15px;
        transition: all 0.5s ease-in-out;
        /* background-color: #000; */
    }

    .nav-open .menu-icon {
        display: none;
        
        transition: all 0.5s ease-in-out;

    }
}
