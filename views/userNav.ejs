    text-transform: uppercase;
    padding-left: 5rem;
    font-family: Gilroy-Regular;
    letter-spacing: 3px;
    margin-top: 20px;

}

.menu>li>a {
    color: white;
    font-weight: 200;
    text-decoration: none;

}

.menu a:hover,
.menu a.active,
.menu a.active {
  opacity: 1;
}
.menu a.inactive {
    opacity: 0.3;
   }

  
  .menu> * {
    transition: opacity 0.3s ease-in;
  }
  
  .menu:hover> * {
    opacity: 0.3;
  }
  
  .menu> *:hover {
    opacity: 1;
  }
